{
  "extends": "eslint-config-egg",
  "rules": {
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "no-unused-vars": [1, {
      "vars": "local",
      "args": "none"
    }],
    // 关闭语句强制分号结尾
    "semi": [0],
    //空行最多不能超过100行
    "no-multiple-empty-lines": [0, {"max": 100}],
    //关闭禁止混用tab和空格
    "no-mixed-spaces-and-tabs": [0]
  }
}
